<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wisdomconsumption">

    <application
        android:name="com.example.base_lib.base.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_circle"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainStartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.MainActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".activity.AccountManagerActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".activity.OptionActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".activity.AppUpdateActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".activity.AboutUsActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".activity.ServiceCenterActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".activity.PromotionActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".activity.ScanActivity"
            android:launchMode="singleTop" />
    </application>

</manifest>