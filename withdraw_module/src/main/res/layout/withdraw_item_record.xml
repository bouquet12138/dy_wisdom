<?xml version="1.0" encoding="utf-8"?>
<com.qmuiteam.qmui.layout.QMUILinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/root"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#fff"
    android:gravity="center_vertical"
    android:orientation="vertical"
    android:padding="10dp">


    <TextView
        android:id="@+id/idText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="编号:&#12288;&#12288;&#12288;&#12288;" />

    <TextView
        android:id="@+id/moneyText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="提现金额:&#12288;&#12288;" />


    <TextView
        android:id="@+id/timeText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="提现时间:&#12288;&#12288;" />

    <TextView
        android:id="@+id/bandCardText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="银行卡号：&#12288;&#12288;" />

    <TextView
        android:id="@+id/weChartText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="微信：&#12288;&#12288;&#12288;&#12288;" />

    <TextView
        android:id="@+id/alipayText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="支付宝：&#12288;&#12288;&#12288;" />

    <TextView
        android:id="@+id/remainAmount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="剩余积分：&#12288;" />

    <TextView
        android:id="@+id/remarkText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="备注:&#12288;&#12288;&#12288;&#12288;" />

    <TextView
        android:id="@+id/isProcessText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="是否处理:&#12288;&#12288;" />

    <TextView
        android:id="@+id/handleTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="处理时间:&#12288;&#12288;" />

</com.qmuiteam.qmui.layout.QMUILinearLayout>